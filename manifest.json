{
    "manifest_version": 3,
    "name": "Bookmark New Tab",
    "version": "1.0",
    "description": "Display all bookmarks on new tab page",
    "permissions": [
      "bookmarks"
    ],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "16": "icon.png"
    }
  }